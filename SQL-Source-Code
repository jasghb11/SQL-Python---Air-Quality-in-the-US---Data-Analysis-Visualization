WHERE ["State"] = '"District of Columbia"' OR ["State"] = '"New York"' OR ["State"] = '"Maryland"' OR
["State"] = '"Pennslyvania"' OR ["State"] = '"Delaware"' OR ["State"] = '"Virginia"' 
OR ["State"] = '"New Jersey"';

